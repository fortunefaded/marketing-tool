{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install --production=false",
  "devCommand": "npm run dev",
  "build": {
    "env": {
      "NODE_ENV": "production",
      "VERCEL": "1",
      "CI": "true"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "ignoreCommand": "git diff HEAD^ HEAD --quiet ."
}
