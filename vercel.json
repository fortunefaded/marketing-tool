{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm ci --include=dev",
  "devCommand": "npm run dev",
  "build": {
    "env": {
      "NODE_ENV": "production",
      "VERCEL": "1",
      "CI": "true"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
